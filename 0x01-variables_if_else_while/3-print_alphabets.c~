#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
 * main - Entry point
 * Description: 'Printing the alphabet '
 * Return: Always 0 (Success)
 */

int main(void)
{
char a = 'a';
char A = 'A'; 
while (a <= 'z' && A <= 'Z')
{
putchar(a);
putchar(A); 
a++;
A++;;
}
putchar('\n');
return (0);
}
