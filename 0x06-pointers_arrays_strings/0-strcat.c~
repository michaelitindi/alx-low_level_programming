#include "main.h"

/**
 * *_strcat - Entry point
 * @dest: pointer variable
 * @src: pointer variable
 * Description: function that concatenates two strings
 * Return: char
 */

char *_strcat(char *dest, char *src) {
char *ptr = dest;
while (*ptr != '\0')
{
ptr++;
}
while (*src != '\0')
{
*ptr++ = *src++;
}
*ptr = '\0';
return dest;
}
