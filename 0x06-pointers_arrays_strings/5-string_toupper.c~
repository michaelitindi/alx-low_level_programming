#include "main.h"

/**
 * *string_toupper - Entry point
 * @str: pointer variable
 * Description: function that changes string to upper case
 * Return: pointer
 */

char *string_toupper(char *str) {
int i = 0;
while (str[i] != '\0')
{
if (str[i] >= 'a' && str[i] <= 'z')
{
str[i] = str[i] - 32;
}
i++;
}

return (str);
}
