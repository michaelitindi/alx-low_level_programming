#include "main.h"

/**
 * *_memset - Entry point
 * @s: pointer
 * @b: character
 * @n: integer
 * Description: function that fills memorry with a constant byte
 * Return: pointer
 */

char *_memset(char *s, char b, unsigned int n)
{
int i;
for (i = 0; i < n; i++)
{
  s[i] = b;
}
return (s);
}
    
