#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "hash_tables.h"





int hash_table_set(hash_table_t *ht, const char *key, const char *value)
{
unsigned long int i;
hash_node_t *new;
hash_node_t *current;
if (ht == NULL || key == NULL || *key == '\0')
{
return (0);
}
i = key_index((const unsigned char *)key, ht->size);
new = malloc(sizeof(hash_node_t));
if (new == NULL)
{
return (0);
}






}
